    "license": "proprietary",
    "minimum-stability": "dev",
    "keywords": ["nws", "Microsoft", "Teams", "webhook", "connector"],
    "authors": [
        {
            "name": "K9 Barry",
            "email": "k9barry@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-mysqli": "*",
        "ext-pdo_mysql": "*",
        "ext-pdo_sqlite": "*",
        "sebbmeyer/php-microsoft-teams-connector": "dev-bugfix",
        "monolog/monolog": "^2.1",
        "k9barry/webhook": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "k9barry\\Webhook\\": "./src/Webhook/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:k9barry/php-microsoft-teams-connector"
